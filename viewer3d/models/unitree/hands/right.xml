<mujoco model="unitree right hand">
  <compiler angle="radian" meshdir="assets"/>

  <default>
    <site rgba="1 0 0 1" size="0.01" group="5"/>

    <default class="visual">
        <geom group="2" type="mesh" contype="0" conaffinity="0" density="0" material="metal"/>
    </default>

    <default class="collision">
        <geom group="3" type="mesh"/>
    </default>
  </default>

  <asset>
    <material name="metal" rgba="0.7 0.7 0.7 1"/>

    <mesh file="right_rubber_hand.STL"/>
  </asset>

  <worldbody>
    <body name="hand" quat="0 0.707 0 .707" gravcomp="1">
        <geom class="visual" mesh="right_rubber_hand"/>
        <geom class="collision" mesh="right_rubber_hand"/>
        <site name="tcp" pos="0.07 0.02 0" quat="0.707 -0.707 0 0"/>
    </body>
  </worldbody>
</mujoco>
