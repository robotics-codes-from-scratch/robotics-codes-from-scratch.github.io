                      <geom class="2f85_visual" pos="0.0127 0.06142 0.01205" quat="1 1 0 0" type="mesh"  mesh="gripper_spring_link"/>
                      <geom class="2f85_collision" pos="0.0127 0.06142 0.01205" quat="1 1 0 0" type="mesh"  mesh="gripper_spring_link"/>
                      <body name="left_follower" pos="-0.0382079 -0.0425003 0.00295" quat="0 -1 -1.90231e-05 0">
                        <inertial mass="0.0125222" pos="0 -0.011046 0.0124786" quat="1 0.1664 0 0"
