                        <geom class="2f85_visual" pos="0.0509079 -0.10392 -0.0091" quat="1 -1 0 0" type="mesh"  mesh="gripper_follower"/>
                        <geom class="2f85_visual" pos="0.0509079 -0.10392 -0.0091" quat="1 -1 0 0" type="mesh" material="metal" mesh="gripper_tongue"/>
                        <geom class="2f85_collision" pos="0.0509079 -0.10392 -0.0091" quat="1 -1 0 0" type="mesh"  mesh="gripper_follower"/>
                        <geom class="2f85_collision" pos="0.0509079 -0.10392 -0.0091" quat="1 -1 0 0" type="mesh"  mesh="gripper_tongue"/>
