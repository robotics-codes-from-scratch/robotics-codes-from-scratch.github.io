                <geom class="collision" size="0.04 0.06" pos="0 0 0.04"/>
                <geom class="collision" pos="0 0.02 0.1" quat="1 1 0 0" size="0.04 0.04"/>
                <body name="wrist_3_link" pos="0 0 0.1" gravcomp="1">
                  <inertial mass="0.1889" pos="0 0.0771683 0" quat="1 0 0 1"
                    diaginertia="0.000132134 9.90863e-05 9.90863e-05"/>
                  <joint name="wrist_3_joint" class="size1"/>
                  <geom material="linkgray" mesh="wrist3" class="visual"/>
                  <geom class="eef_collision" pos="0 0.08 0" quat="1 1 0 0" size="0.04 0.02"/>

                  <body name="gripper_base" childclass="2f85" pos="0 0.1 0" quat="-1 1 0 0" gravcomp="1">
                    <inertial mass="0.777441" pos="0 -2.70394e-05 0.0354675" quat="1 -0.00152849 0 0"
                      diaginertia="0.000260285 0.000225381 0.000152708"/>
                    <geom class="2f85_visual" pos="0 0 0.0108" quat="0 0 0 1"   mesh="gripper_base"/>
                    <geom class="2f85_visual" pos="0 0 0.004" quat="1 -1 0 0"   mesh="gripper_base_coupling"/>
                    <geom class="2f85_visual" pos="0 0 0.0108" quat="1 0 0 0"  material="metal" mesh="gripper_c-a01-85-open"/>
                    <geom class="2f85_collision" mesh="gripper_base"/>
                    <site name="tcp" pos="0 0 0.155"/>
                    <!-- Left-hand side 4-bar linkage -->
                    <body name="left_driver" pos="-0.0306011 0.00475 0.0657045" quat="1 -1 0 0">
                      <inertial mass="0.00899563" pos="0 0.0177547 0.00107314" quat="0.681301 0.732003 0 0"
                          diaginertia="1.72352e-06 1.60906e-06 3.22006e-07"/>
                      <joint name="left_driver_joint" class="driver"/>
                      <geom class="2f85_visual" pos="0.0306011 0.0549045 -0.0047" quat="1 1 0 0"  material="metal" mesh="gripper_driver"/>
                      <geom class="2f85_collision" pos="0.0306011 0.0549045 -0.0047" quat="1 1 0 0"   mesh="gripper_driver"/>
                      <body name="left_coupler" pos="-0.0314249 0.00453223 -0.0102" quat="0 0 0 1">
                        <inertial mass="0.0140974" pos="0 0.00301209 0.0232175" quat="0.705636 -0.0455904 0.0455904 0.705636"
                        diaginertia="4.16206e-06 3.52216e-06 8.88131e-07"/>
                        <geom class="2f85_visual" pos="-0.062026 -0.0503723 0.0055" quat="1 -1 0 0"   mesh="gripper_coupler"/>
                        <geom class="2f85_collision" pos="-0.062026 -0.0503723 0.0055" quat="1 -1 0 0"  mesh="gripper_coupler"/>
                      </body>
                    </body>
                    <body name="left_spring_link" pos="-0.0127 -0.012 0.07222" quat="1 -1 0 0">
                      <inertial mass="0.0221642" pos="-8.65005e-09 0.0181624 0.0212658" quat="0.663403 -0.244737 0.244737 0.663403"
                          diaginertia="8.96853e-06 6.71733e-06 2.63931e-06"/>
